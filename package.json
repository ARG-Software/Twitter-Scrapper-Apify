{
  "name": "apify-poc",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "start-dev": "nodemon --exec ts-node src/index.ts",
    "db:stop": "supabase stop",
    "db:start": "supabase start",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase migration up",
    "migration:generate": "mikro-orm migration:create --config mikro-orm.config.ts",
    "migration:up": "mikro-orm migration:up --config mikro-orm.config.ts",
    "migration:down": "mikro-orm migration:down --config mikro-orm.config.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts,json}'"
  },
  "author": "ARG",
  "dependencies": {
    "@mikro-orm/core": "^6.2.9",
    "@mikro-orm/migrations": "^6.2.9",
    "@mikro-orm/postgresql": "^6.2.9",
    "@supabase/supabase-js": "^2.38.2",
    "apify": "^3.2.3",
    "pg": "^8.12.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/jest": "^29.5.12",
    "@types/uuid": "^9.0.8",
    "dotenv": "^16.4.5",
    "eslint": "^9.3.0",
    "globals": "^15.3.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.2",
    "supabase": "^1.93.0",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts"
    ]
  }
}
