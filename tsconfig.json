{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
    },
    "include":["src/**/*"],
    "exclude": ["node_modules", "src/migrations"]
  }
